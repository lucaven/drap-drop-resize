    "@vue/eslint-config-standard": "^3.0.5",
    "babel-plugin-transform-flow-comments": "^6.7.0",
    "node-sass": "^4.13.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.4.1"
